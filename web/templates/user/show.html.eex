<h2>Usuario <%= @user.name %></h2>
<table class="table">
  <tr>
    <th>
      ID
    </th>
    <th>
      Lastname
    </th>
    <th>
      Username
    </th>
    <th>
      Password
    </th>
  </tr>
  <tr>
    <td>
      <%= @user.id %>
    </td>
    <td>
      <%= last_name(@user) %>
    </td>
    <td>
      <%= @user.username %>
    </td>
    <td>
      <%= @user.password %>
    </td>
  </tr>
</table>
